# Ryan Ye's setting
if ($HOST =~ hk-xterm*) then 
	set prompt="%Bhk:%c1>"
else
	set prompt="%hko:%c1>"
endif

#p4
setenv P4CONFIG .p4config
setenv P4DIFF vimdiff
setenv P4EDITOR ~/app/bin/vim
setenv LSB_DEFAULTPROJECT gt218
setenv TRIAGE_SCRIPT_PATH ~/bin/triage
setenv DISPLAY hk-xterm-01:21
setenv RSYNC_DIR hk-xterm-05:/home/rye/spoint

set path = (\
	~/app/bin \
	~/bin \
	/home/scratch.yzhang_tesla2/p4_gt218/hw/class/tools/trace_disassembler/Release \
	/home/tools/j2sdk/Linux/jdk1.5.0/bin/java\
	/home/scratch.rye_tesla2/hw/tools/treebin\
	/home/gnu/wine2004/bin \
	/home/gnu/wine2004/lib \
	/home/gnu/bin /usr/bin/  / home/utils/emacs-21.4/bin/ \
	/home/scratch.rye_tesla2/hw/tools/imgtools/bin \
        $path \
	) 
setenv LD_LIBRARY_PATH "/home/gnu/wine2004/lib:/home/vp2/tensilica/RA-2005.1-linux/XtensaTools/Tools/lib:$LD_LIBRARY_PATH"
setenv MANPATH "/home/rye/app/man:$MANPATH"

alias ctags /usr/bin/ctags
alias vpr "pwd | xargs --replace=A mozilla file://A &"
alias gog86 "cd /home/scratch.g98perfRTL/trees/g86_rtl003/hw/g86/diag/PerfGen"
alias gw "cd /home/scratch.gt218perfWaves/perfgen/daily"
alias gt "cd /home/scratch.gt218perfTrees/trees"
alias grtl "cd /home/scratch.gt218perfRTL/perfgen/gt218"
alias lsw "ls -d /home/scratch.gt218perfWaves/perfgen/daily/*bug\!:1_*"
alias pbsum "pbsum.pl -user \!:*"
alias mytree "cd /home/scratch.rye_tesla2/"
setenv MANPAGER "less"
alias gocatalyze "cd /home/scratch.rye_tesla2/hw/class/tesla/catalyze"

# desc: show CL num. of specific tree
# usage: CL [class num.]
alias CL "cat /home/scratch.gt218perfTrees/trees/gt218_\!:1/PERF_CHANGELISTS"
alias pg "cd hw/tesla2/diag/PerfGen"
alias pt "cd hw/tesla2/perftest/tests/Common"

# for gt218 daily integeration
alias integ "p4 sync ... && p4 integ -b tesla2_gt218_perf -td ... && p4 integ -b tesla2_gt218_perf_compare -td ..."
alias resolve "p4 resolve -am ..."

alias pghistory "cat perfgen_command_line_log"
alias editrc "vim ~/.cshrc_custom; source ~/.cshrc_custom"
alias triagerc "vim $TRIAGE_SCRIPT_PATH/.gt218_triage_rc; source ~/.cshrc_custom"
alias vimrc "vim ~/.vimrc"
alias applyrc "source ~/.cshrc_custom"
alias cphk "scp \!:1 hk-xterm-01:/home/scratch.rye_tesla2"
alias clip '(\!:*) | winclip'

# submit jobs
alias bsub1 'bsub -P $LSB_DEFAULTPROJECT -q l_pri_cpu_2G' # for non-interative job
alias bsub2 'bsub -I -P $LSB_DEFAULTPROJECT -q l_pri_interactive' # for interative job such as nWave

# write notes
alias note "vim ~/notes/\!:1"
alias shownote "cat ~/notes/\!:1 | more"
alias notes "ls ~/notes"
complete {shownote,note} 'C@*@`ls ~/notes`@'

# for vi
alias itable "interface.pl | table"

# dir stack
alias dv "dirs -v"
alias push "pushd ."
alias pop "popd"

set autolist
set autocorrect
set complete = enhance
source $TRIAGE_SCRIPT_PATH/.gt218_triage_rc

